/*
	tswtabs.css		
	display horizontal menu bar for the cell phone.
*/


/* set the unordered list default setting 
This is the horizontal menu bar setting
*/
#tswcsstabs ul {
	margin: 0 ;
	padding: 0 ;
	list-style: none ;
	display: inline ;
}
/* set the menu item default setting */ 
#tswcsstabs ul li {
	margin: 0 ;
	padding: 0 ;
	display: inline ;
	text-align: center ;
	float: left ;
	font-family: Arial, Helvetica, sans-serif ;
}
/* set the menu item link default setting */ 
#tswcsstabs li a {
	color: #000000 ;
	background-color: #ffe500 ;
	border: 1px outset #b3a000 ;
	padding: 8px ;
	text-decoration: none ;
	display: inline ;
}
/* when the mouse is over the menu item,
change text color of the link to yellow
*/
#tswcsstabs li a:hover {
	color: #000000 ;
	background-color: #b3a000 ;
}
